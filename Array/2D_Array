#include<iostream>
using namespace std;
int main(){
    int n,m;
    cout<<"enter row:";
    cin>>n;
    cout<<"enter col:";
    cin>>m;
    int a[n][m];
    cout<<"enter the elements of array:";
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            cin>>a[i][j];
        }
    }
    cout<<"2D array is:"<<endl;
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0;
}
