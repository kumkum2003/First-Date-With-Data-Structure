#include<bits/stdc++.h>
using namespace std;
void call_by_reference_address(int& n)
{
	cout<<"adress of n in call_by_reference_address function: "<<&n<<endl;
	n*=n;

}
int main()
{
	int n=8;
	cout<<"adress of n in main : "<<&n<<endl;
	call_by_reference_address(n);
	cout<<"ans: "<<n<<endl;
	cout<<"value of n: "<<n<<endl;
 return 0;
	
}
